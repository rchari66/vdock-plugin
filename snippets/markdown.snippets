# scope: markdown

snippet vheading1
	# ${1:Heading1}

snippet vheading2
	## ${1:Heading2}

snippet vbold
	**${1:BOLD}**

snippet vitalic
	_${1:Italic}_

snippet vlist
	* ${1:List Item 1}
	* ${2:List Item 2}

snippet vlistUnordered
	* ${1:List Item 1}
	* ${2:List Item 2}
		* ${3:sub list item 1}
		* ${4:sub list item 2}

snippet vtest
	${1:this is test}
